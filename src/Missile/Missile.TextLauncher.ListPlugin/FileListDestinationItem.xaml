<UserControl x:Class="Missile.TextLauncher.ListPlugin.FileListDestinationItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Missile.TextLauncher.ListPlugin"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel Orientation="Horizontal">
        <Image x:Name="IconImage"></Image>
        <StackPanel>
            <TextBlock x:Name="FileNameTextBlock"></TextBlock>
            <TextBlock x:Name="FilePathTextBlock"></TextBlock>
        </StackPanel>            
    </StackPanel>
</UserControl>
